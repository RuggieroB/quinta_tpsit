<!DOCTYPE html>

<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<title>ToDos App</title>
	</head>

	<body>
		<h1>ToDos App</h1>
		<ul>
			<% todos.forEach( function (todo) { %>
				<li>
					<%= todo.text %> - <%= todo.done %>
				</li>
			<% }); %>
		</ul>
	</body>
</html>
